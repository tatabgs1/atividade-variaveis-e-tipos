var num;
var array1 = [1, 3, 4, 6, 80, 33, 23, 90];

function testaPar(array) {

    for (let i = 0; i < array1.length; i++) {
    if (array1[i] % 2 === 0){
        array1[i] = 0;
       
    }
    else if (array[i]==0) {
        console.log('Ops, você já vale 0!');}
    
}
console.log(array1);
    
}
testaPar([1, 3, 4, 6, 80, 33, 23, 90]);
